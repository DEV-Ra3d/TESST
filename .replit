run = "python main.py"
modules = ["python-3.10:v18-20230807-322e88b"]

[nix]
channel = "stable-23_05"

[deployment]
run = ["sh", "-c", "python main.py"]

[[ports]]
localPort = 8080
externalPort = 80

[languages]

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "main.py"]

[env]
PYTHONPATH = "/home/runner/$REPL_SLUG"
PYTHONUNBUFFERED = "1"
